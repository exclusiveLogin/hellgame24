{
  "name": "hg24",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run doc && ng build --prod --stats-json && cp backend/ -r dist/",
    "doc": "rm src/wiki/* -rf && typedoc --out ./src/wiki ./src/app/ --options typedoc.json --exclude \"**/*+(node_modules|modules|libs)/**/*\" --excludeExternals --externalPattern \"**/*+(node_modules|modules|libs)/**/*\" --baseUrl /wiki/ ",
    "docker:build": "sudo npm i && sudo docker build -t exclusivelogin/hellgame24-front .",
    "docker:publish": "npm run docker:build && sudo docker login && sudo docker push exclusivelogin/hellgame24-front",
    "docker:run": "sudo docker pull exclusivelogin/birthhelper-front && sudo docker run --rm -d -p 8094:80 exclusivelogin/hellgame24-front",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "angular-highcharts": "^8.0.3",
    "core-js": "^2.4.1",
    "highcharts": "^9.0.1",
    "moment": "^2.24.0",
    "ng-smoothscroll": "^1.8.0",
    "ng5-slider": "^1.1.13",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "tslib": "^1.9.0",
    "typedoc": "^0.14.2",
    "ua-parser-js": "^0.7.19",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular/cli": "8.3.29",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/highcharts": "^5.0.22",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "3.5.3"
  }
}
