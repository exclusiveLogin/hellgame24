{
  "name": "hg24",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run doc && ng build --prod --stats-json && cp backend/ -r dist/",
    "doc": "rm src/wiki/* -rf && typedoc --out ./src/wiki ./src/app/ --options typedoc.json --exclude \"**/*+(node_modules|modules|libs)/**/*\" --excludeExternals --externalPattern \"**/*+(node_modules|modules|libs)/**/*\" --baseUrl /wiki/ ",
    "docker:build": "sudo npm i && sudo docker build -t exclusivelogin/hellgame24-front .",
    "docker:publish": "npm run docker:build && sudo docker login && sudo docker push exclusivelogin/hellgame24-front",
    "docker:run": "sudo docker pull exclusivelogin/birthhelper-front && sudo docker run --rm -d -p 8094:80 exclusivelogin/hellgame24-front",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.4",
    "@angular/common": "10.2.4",
    "@angular/compiler": "10.2.4",
    "@angular/core": "10.2.4",
    "@angular/forms": "10.2.4",
    "@angular/platform-browser": "10.2.4",
    "@angular/platform-browser-dynamic": "10.2.4",
    "@angular/router": "10.2.4",
    "angular-highcharts": "^11.3.1",
    "core-js": "^2.4.1",
    "highcharts": "^9.0.1",
    "moment": "^2.24.0",
    "ng-smoothscroll": "^1.8.0",
    "ng5-slider": "^1.1.13",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "tslib": "^2.0.0",
    "typedoc": "^0.14.2",
    "ua-parser-js": "^0.7.19",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "10.2.3",
    "@angular/compiler-cli": "10.2.4",
    "@angular/language-service": "10.2.4",
    "@types/highcharts": "^5.0.22",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~4.1.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.7"
  }
}
